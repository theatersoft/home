{
  "name": "home",
  "version": "1.0.1",
  "description": "Theatersoft Home",
  "author": "Rob Huehn <rob@theatersoft.com>",
  "license": "AGPL-3.0",
  "private": true,
  "scripts": {
    "link": "npm link @theatersoft/server",
    "config": "node deploy config",
    "debug-config": "node --inspect-brk deploy config"
  },
  "deployScripts": {
    "adduser": "sudo adduser --system --group --disabled-password theatersoft",
    "start": "XDG_CONFIG_HOME=/opt/theatersoft/.config npm explore @theatersoft/server npm run start",
    "start-capture": "XDG_CONFIG_HOME=/opt/theatersoft/.config npm explore @theatersoft/server npm run start-capture"
  },
  "devDependencies": {
    "shelljs": "^0.7.0"
  },
  "dependencies": {
    "@theatersoft/client": "/home/rob/Theatersoft/client/dist",
    "@theatersoft/server": "/home/rob/Theatersoft/server/dist"
  }
}